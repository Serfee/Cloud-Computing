{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": [
            "error",
            4//->default 2
        ],
        "quotes": [
            "error",
            "double"
        ],
        "linebreak-style": 0, //->0
        "no-trailing-spaces": "off",//->on/off
        "no-console": "off", //->off
        "comma-dangle": "off", //->off
        "eol-last": 0, //->0
        "prefer-destructuring": "off"
    }
    
}
